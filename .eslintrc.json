{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "no-return-assign": "off",
    "node/no-unsupported-features/es-syntax": "off",
    "no-param-reassign": "off",
    "node/no-unpublished-import": "off"
  }
}
